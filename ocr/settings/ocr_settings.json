{
  "default_engine": "ocrmypdf",
  "engines": {
    "ocrmypdf": {
      "deskew": false,
      "rotate_pages": false,
      "remove_background": false,
      "clean": false,
      "oversample": 300
    },
    "tesseract": {
      "psm": "3",
      "oem": "3",
      "lang": "jpn+eng",
      "dpi": 600,
      "preserve_interword_spaces": true,
      "tessedit_char_whitelist": "",
      "tessedit_char_blacklist": "",
      "classify_bln_numeric_mode": false,
      "textord_really_old_xheight": false,
      "textord_min_linesize": 1.25,
      "tosp_threshold_bias1": 1.5,
      "tosp_threshold_bias2": 0,
      "edges_max_children_per_outline": 10,
      "edges_children_per_grandchild": 2,
      "language_model_penalty_non_freq_dict_word": 0.1,
      "language_model_penalty_case": 0.1
    },
    "easyocr": {
      "languages": "ja",
      "use_gpu": false,
      "zoom_factor": 4,
      "detail": "0",
      "paragraph": false,
      "high_quality_mode": true,
      "text_threshold": 0.7,
      "link_threshold": 0.4,
      "low_text": 0.4,
      "canvas_size": 2560,
      "mag_ratio": 1.5
    },
    "paddleocr": {
      "lang": "japan",
      "use_gpu": false,
      "zoom_factor": 2.0,
      "use_angle_cls": true,
      "use_detection": true,
      "use_recognition": true,
      "use_classification": true,
      "show_log": false
    }
  },
  "global": {
    "timeout": 300,
    "max_retries": 3
  }
}